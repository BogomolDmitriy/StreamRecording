<Window x:Class="StreamRecording.RecordingStartWindow.TestURL.TestUrlWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StreamRecording.RecordingStartWindow.TestURL"
        mc:Ignorable="d"
        Title="TestUrlWindow" Height="400" Width="310"
        ResizeMode="NoResize">
    <Grid>
        <StackPanel  HorizontalAlignment="Center" VerticalAlignment="Center" Orientation ="Vertical">
            <DataGrid x:Name="TestURLDataGrid" AutoGenerateColumns="True" Width="280" Height="300" />
            <Button x:Name ="buttonClose"  Content="Close" Width="100" Height="25" Click="Close_Click" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="5,10,10,10" />
        </StackPanel>
        
    </Grid>
</Window>
